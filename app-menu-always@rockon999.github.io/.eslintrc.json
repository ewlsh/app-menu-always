{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-magic-numbers": [
            1,
            {
                "ignore": [
                    -1,
                    0,
                    1,
                    2,
                    255
                ],
                "enforceConst": true
            }
        ],
        "no-with": 2,
        "no-delete-var": 1,
        "no-unused-vars": [
            2,
            {
                "args": "none"
            }
        ],
        "eqeqeq": ["error", "always", { "null": "ignore" }]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "globals": {
        "imports": false,
        "global": false,
        "log": false
    },
    "extends": "eslint:recommended"
}